export PATH=$(brew --prefix coreutils)/libexec/gnubin:/usr/local/sbin:$GOROOT/bin:${HOME}/bin:${PATH}

# Update PATH for the Google Cloud SDK
if [[ ${SHELL} == *"zsh" ]]; then
  source '/Users/cgwong/repos/workspace/gcloud/google-cloud-sdk/path.zsh.inc'
else
  source '/Users/cgwong/repos/workspace/gcloud/google-cloud-sdk/path.bash.inc'
fi

# Add rbenv to PATH so that it loads every time you open a terminal
export PATH="${HOME}/.rbenv/bin:${PATH}"
