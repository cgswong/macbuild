# Disguise as IE 9 on Windows 7.
##user-agent = "Mozilla/5.0 (compatible; MSIE 9.0; Windows NT 6.1; Trident/5.0)"

# When following a redirect, automatically set the previous URL as referer.
referer = ";auto"

# Wait 60 seconds before timing out.
connect-timeout = 30
# Display progress bar instead of informal meter
progress-bar
# Follow redirects
location
# Make insecure connections the default. Required on internal work network due to man-in-the-middle SSL inspection
insecure
